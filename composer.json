{
    "name": "faridibin/laravel-api-response",
    "description": "Easy Api responses for laravel.",
    "keywords": ["laravel", "api", "response", "json", "xml"],
    "license": "MIT",
    "authors": [{
        "name": "Farid Adam",
        "email": "faridibin@gmail.com"
    }],
    "require": {
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "nunomaduro/collision": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Faridibin\\LaravelApiResponse\\": "src/"
        },
        "files": [
            "src/Helpers/constants.php",
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Faridibin\\LaravelApiResponse\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faridibin\\LaravelApiResponse\\ApiResponseServiceProvider"
            ]
        }
    }
}
